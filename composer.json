{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": [
        "yii2",
        "framework",
        "basic",
        "project template"
    ],
    "homepage": "https://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "ircs://irc.libera.chat:6697/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4.0",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap5": "~2.0.2",
        "yiisoft/yii2-symfonymailer": "~2.0.3",
        "yiisoft/yii2-queue": "^2.3",
        "vlucas/phpdotenv": "^5.5",
        "psr/log": "^3.0",
        "kartik-v/yii2-widget-select2": "^2.2",
        "giggsey/libphonenumber-for-php": "^9.0",
        "yiisoft/yii2-redis": "^2.1",
        "guzzlehttp/guzzle": "^7.10",
        "inspector-apm/inspector-php": "^3.16"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0 || ^7.0",
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^5.0.0 || ^4.0",
        "codeception/lib-innerbrowser": "^4.0 || ^3.0 || ^1.1",
        "codeception/module-asserts": "^3.0 || ^1.1",
        "codeception/module-yii2": "^1.1",
        "codeception/module-filesystem": "^3.0 || ^2.0 || ^1.1",
        "codeception/module-webdriver": "^4.0 || ^3.0 || ^1.2",
        "codeception/verify": "^3.0 || ^2.2",
        "squizlabs/php_codesniffer": "^4.0",
        "yiisoft/yii2-coding-standards": "^3.0",
        "slevomat/coding-standard": "^8.25",
        "phpstan/phpstan": "^2.1",
        "deptrac/deptrac": "^3.0",
        "infection/infection": "^0.32.0",
        "infection/codeception-adapter": "^0.4.4",
        "rector/rector": "^2.3",
        "zircote/swagger-php": "^5.0",
        "codeception/module-rest": "^3.4",
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "buggregator/trap": "^1.15",
        "phpro/grumphp": "^2.18"
    },
    "autoload": {
        "psr-4": {
            "app\\": "",
            "Tools\\": "tools/"
        },
        "files": [
            "tools/PHPUnit/Framework/Attributes/CodeCoverageIgnore.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true,
            "phpro/grumphp": true
        },
        "process-timeout": 1800,
        "default-socket-timeout": 120,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org",
            "options": {
                "http": {
                    "timeout": 120
                }
            }
        }
    ]
}
